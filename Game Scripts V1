local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Game Scripts", "Sentinel")

warn("Scripts\
\
Pet Simulator X\
Raise A Floppa\
Raise A Floppa2\
Stand Upright\
Da Hood\
A Universal Time\
Ninja Legends\
Stands Awakening\
Blox Fruits\
Build A Boat For a Treasure\
Murder Mystery 2")


-- Scripts
local Scripts = Window:NewTab("Scripts")
local PetSimXSection = Scripts:NewSection("Pet Simulator X Scripts")

PetSimXSection:NewButton("Project Meow", "PetSimX Script 1", function()
    loadstring(game:HttpGet("https://rawscripts.net/raw/Project-Meow_421"))()
end)

PetSimXSection:NewButton("Delta PSX Script", "PetSimX Script 2", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/lxnnydev/Delta-PSX/main/loader.lua"))()
end)

local RFPS = Scripts:NewSection("Raise A Floppa Scripts")

RFPS:NewButton("RFP GUI", "RFP Script 1", function()
    _G.Theme = "Dark"
    loadstring(game:HttpGet("https://raw.githubusercontent.com/CasperFlyModz/discord.gg-rips/main/raiseafloppa.lua"))()
end)

