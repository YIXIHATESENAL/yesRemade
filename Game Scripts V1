-- Settings
local settings = Window:NewTab("Settings")
local settingss = settings:NewSection("Settings")
local credits = settings:NewSection("Credits")

settingss:NewKeybind("Toggle UI", "", Enum.KeyCode.L, function()
	Library:ToggleUI()
end)

credits:NewButton("YT", "my YT CHANNEL", function()
    setclipboard("https://www.youtube.com/channel/UCIUxqEsP1CpT-nJHk_veP2Q")
    game:GetService("StarterGui"):SetCore("SendNotification",{
        Title = "YT Channel",
        Text = "Copied To Clipboard",
        Duration = 7,
        Button1 = "add more scripts pls"
    })
end)

credits:NewButton("Roblox Acc", "my roblox accounts", function()
    game:GetService("StarterGui"):SetCore("SendNotification",{
        Title = "Ihatearsenal1233",
        Text = "acc 1",
        Duration = 5,
        Button1 = "yes"
    })
    wait(1)
    game:GetService("StarterGui"):SetCore("SendNotification",{
        Title = "YIXINKYI",
        Text = "acc 2",
        Duration = 5,
        Button1 = "yes"
    })
end)

